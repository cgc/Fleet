include ../../Fleet.mk

all:
	g++ -Wall -fdiagnostics-color=auto -o main Main.cpp -lm -O2 $(FLEET_FLAGS) $(FLEET_INCLUDE)

debug:
	g++-9 -Wall -fdiagnostics-color=auto -o main Main.cpp -lm -g -O2 $(FLEET_FLAGS) $(FLEET_INCLUDE)
	
postprocess:
	g++-9 -Wall -fdiagnostics-color=auto -o pp Postprocess.cpp -lm -O2 $(FLEET_FLAGS) $(FLEET_INCLUDE)
